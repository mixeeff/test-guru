<tr>
  <td><%= link_to test.title, admin_test_path(test) %> </td>
  <td> <%= test.category.title %> </td>
  <td align="center"> <%= test.level %> </td>
  <td align="center"> <%= test.questions.count %> </td>
  <td> <%= link_to 'Редактировать', edit_admin_test_path(test) %> </td>
  <td> <%= link_to 'Удалить', admin_test_path(test), method: :delete, data: { confirm: 'Вы уверены?' } %> </td>
  <td> <%= button_to 'Старт', start_test_path(test) %> </td>
</tr>
