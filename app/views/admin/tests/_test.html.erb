<tr>
  <td><%= link_to test.title, admin_test_path(test) %> </td>
  <td> <%= test.category.title %> </td>
  <td align="center"> <%= test.level %> </td>
  <td align="center"> <%= test.questions.count %> </td>
  <td> <%= link_to t('.edit'), edit_admin_test_path(test) %> </td>
  <td> <%= link_to t('.delete'), admin_test_path(test), method: :delete, data: { confirm: t('.confirm') } %> </td>
  <td> <%= button_to t('.start'), start_test_path(test) %> </td>
</tr>
