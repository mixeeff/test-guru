<tr>
  <td> <% if logged_in? %>
        <%= link_to test.title, test_path(test) %> </td>
      <% else %>
        <%= test.title %>
      <% end %>
  <td> <%= test.category.title %> </td>
  <td align="center"> <%= test.level %> </td>
  <td align="center"> <%= test.questions.count %> </td>
  <% if logged_in? %>
    <td> <%= link_to 'Редактировать', edit_test_path(test) %> </td>
    <td> <%= link_to 'Удалить', test_path(test), method: :delete, data: { confirm: 'Вы уверены?' } %> </td>
    <td> <%= button_to 'Старт', start_test_path(test) %> </td>
  <% end %>
</tr>
